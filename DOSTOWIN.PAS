program dostowin;
Const
  Dos2WinTable:Array[0..127] of char =
               (#192,#193,#194,#195,#196,#197,#198,#199,#200,#201,#202,#203,
                #204,#205,#206,#207,#208,#209,#210,#211,#212,#213,#214,#215,
                #216,#217,#218,#219,#220,#221,#222,#223,#224,#225,#226,#227,
                #228,#229,#230,#231,#232,#233,#234,#235,#236,#237,#238,#239,
                #127,#127,#127,#127,#127,#127,#127,#127,#127,#127,#127,#127,
                #127,#127,#127,#127,#127,#127,#127,#127,#173,#127,#127,#127,
                #127,#127,#127,#127,#127,#127,#127,#127,#285,#129,#189,#178,
                #163,#138,#140,#142,#141,#127,#143,#144,#131,#190,#179,#188,
                #240,#241,#242,#243,#244,#245,#246,#247,#248,#249,#250,#251,
                #252,#253,#254,#255,#168,#184,#170,#186,#175,#191,#161,#162,
                #154,#156,#158,#157,#185,#159,#167,#160);

Var
  S:string;

Function Dos2Win(S:String):String;
Var i:integer;
begin
   For i:=1 to length(S) do begin
      If S[i]>#127 then
         S[i]:=Dos2WinTable[Byte(S[i])-127];
   end;
   Dos2Win:=S;
end;


begin
S:='1';
Writeln('Dos to Win convert');
While S <> '' do begin
   Write('Input: ');
   Readln(S);
   Writeln('Output:',Dos2Win(S));
end;
end.
